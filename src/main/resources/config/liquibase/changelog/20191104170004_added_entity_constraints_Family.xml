<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Family.
    -->
    <changeSet id="20191104170004-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="family_id"
                                 baseTableName="family_elderly"
                                 constraintName="fk_family_elderly_family_id"
                                 referencedColumnNames="id"
                                 referencedTableName="family"/>

        <addForeignKeyConstraint baseColumnNames="elderly_id"
                                 baseTableName="family_elderly"
                                 constraintName="fk_family_elderly_elderly_id"
                                 referencedColumnNames="id"
                                 referencedTableName="elderly"/>
        
    </changeSet>
</databaseChangeLog>
