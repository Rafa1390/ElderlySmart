<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Elderly.
    -->
    <changeSet id="20191104165955-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="asylum_id"
                                 baseTableName="elderly"
                                 constraintName="fk_elderly_asylum_id"
                                 referencedColumnNames="id"
                                 referencedTableName="asylum"/>

        <addForeignKeyConstraint baseColumnNames="employee_id"
                                 baseTableName="elderly"
                                 constraintName="fk_elderly_employee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

    </changeSet>
</databaseChangeLog>
