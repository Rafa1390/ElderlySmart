<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Mortuary.
    -->
    <changeSet id="20191104170005-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_app_id"
                                 baseTableName="mortuary"
                                 constraintName="fk_mortuary_user_app_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_app"/>

        <addForeignKeyConstraint baseColumnNames="mortuary_id"
                                 baseTableName="mortuary_asylum"
                                 constraintName="fk_mortuary_asylum_mortuary_id"
                                 referencedColumnNames="id"
                                 referencedTableName="mortuary"/>

        <addForeignKeyConstraint baseColumnNames="asylum_id"
                                 baseTableName="mortuary_asylum"
                                 constraintName="fk_mortuary_asylum_asylum_id"
                                 referencedColumnNames="id"
                                 referencedTableName="asylum"/>
        
    </changeSet>
</databaseChangeLog>
